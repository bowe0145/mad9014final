<?xml version='1.0' encoding='utf-8'?>
<widget id="ca.edumedia.bowe0145.bowe0145sportsApp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="SplashScreen">
        <param name="android-package" value="org.apache.cordova.splashscreen.SplashScreen" />
        <param name="onload" value="true" />
    </feature>
    <name>bowe0145sportsApp</name>
    <description>
        A Final Project for mad9014
    </description>
    <author email="bowe0145@algonquinlive.com" href="http://bowe0145.edumedia.ca">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <splash density="land-mdpi" src="res/screen/Landscape-MDPI.svg" />
    <splash density="land-hdpi" src="res/screen/Landscape-HDPI.svg" />
    <splash density="land-xhdpi" src="res/screen/Landscape-XHDPI.svg" />
    <splash density="land-xxhdpi" src="res/screen/Landscape-XXHDPI.svg" />
    <splash density="land-xxxhdpi" src="res/screen/Landscape-XXXHDPI.svg" />
    <splash density="port-mdpi" src="res/screen/Portrait-MDPI.svg" />
    <splash density="port-hdpi" src="res/screen/Portrait-HDPI.svg" />
    <splash density="port-xhdpi" src="res/screen/Portrait-XHDPI.svg" />
    <splash density="port-xxhdpi" src="res/screen/Portrait-XXHDPI.svg" />
    <splash density="port-xxxhdpi" src="res/screen/Portrait-XXXHDPI.svg" />
    <preference name="loglevel" value="DEBUG" />
</widget>
