<?xml version='1.0' encoding='utf-8'?>
<widget id="ca.edumedia.bowe0145.bowe0145sportsApp" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>bowe0145sportsApp</name>
    <description>
        A Final Project for mad9014
    </description>
    <author email="bowe0145@algonquinlive.com" href="http://bowe0145.edumedia.ca">
        Apache Cordova Team
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
            <splash src="res/screen/Landscape-MDPI.svg" density="land-mdpi"/>
            <splash src="res/screen/Landscape-HDPI.svg" density="land-hdpi"/>
            <splash src="res/screen/Landscape-XHDPI.svg" density="land-xhdpi"/>
        <splash src="res/screen/Landscape-XXHDPI.svg" density="land-xxhdpi"/>
        <splash src="res/screen/Landscape-XXXHDPI.svg" density="land-xxxhdpi"/>

            <splash src="res/screen/Portrait-MDPI.svg" density="port-mdpi"/>
            <splash src="res/screen/Portrait-HDPI.svg" density="port-hdpi"/>
        <splash src="res/screen/Portrait-XHDPI.svg" density="port-xhdpi"/>
        <splash src="res/screen/Portrait-XXHDPI.svg" density="port-xxhdpi"/>
        <splash src="res/screen/Portrait-XXXHDPI.svg" density="port-xxxhdpi"/>
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
